{
    "manifest_version": 3,
    "name": "Fasty flavortown extension",
    "version": "1.3",
    "description": "Faster expirience",
    "permissions": [
        "declarativeNetRequest"
    ],
    "declarative_net_request": {
        "rule_resources": [
            {
                "id": "ruleset_1",
                "enabled": true,
                "path": "rules.json"
            }
        ]
    },
    "host_permissions": [
        "*://flavortown.hackclub.com/*"
    ],
    "browser_specific_settings": {
        "gecko": {
            "id": "flavortown-ext@hackclub.com"
        }
    },
    "content_scripts": [
        {
            "matches": [
                "*://flavortown.hackclub.com/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_start"
        }
    ],
    "icons": {
        "48": "icon.png"
    }
}